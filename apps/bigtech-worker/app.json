        "CFBundleDisplayName": "大厂生存指南",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1E88E5"
      },
      "package": "com.themecardgames.bigtechworker"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "bigtech-worker",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "989900d3-1088-454a-b37e-99c7ea679014"
      }
    },
    "owner": "wuxiupengftd"
  }
